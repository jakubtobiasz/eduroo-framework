{
    "name": "eduroo/framework",
    "description": "E-learning Framework unleashing the power of Open-Source",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Tobiasz",
            "email": "jakub@eduroo.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eduroo\\Component\\": "components/",
            "Eduroo\\Component\\Extension\\": "components/Extension/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "require-dev": {
        "captainhook/captainhook": "^5.16",
        "mockery/mockery": "^1.5",
        "pestphp/pest": "^2.2",
        "phpstan/phpstan": "^1.10",
        "qossmic/deptrac-shim": "^1.0",
        "symplify/easy-coding-standard": "^11.3",
        "symplify/monorepo-builder": "11.2.2.72",
        "vimeo/psalm": "^5.8"
    },
    "replace": {
        "eduroo/extension": "self.version"
    }
}
